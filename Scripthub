-- Fonction pour récupérer le nom d'utilisateur
local function getUsername()
    local success, result = pcall(function()
        return game.Players:GetNameFromUserIdAsync(game.Players.LocalPlayer.UserId)
    end)
    if success then
        return result
    else
        return game.Players.LocalPlayer.Name
    end
end

-- Récupérer le nom d'utilisateur actuel
local playerName = getUsername()

-- Vérifier si le nom d'utilisateur est égal à "3encxwd"
if playerName == "3encxwd" then
    -- Si c'est "3encxwd", afficher un beau GUI
    local messageGui = Instance.new("ScreenGui")
    local messageFrame = Instance.new("Frame", messageGui)
    messageFrame.Size = UDim2.new(0.5, 0, 0.5, 0)
    messageFrame.Position = UDim2.new(0.25, 0, 0.25, 0)
    messageFrame.BackgroundColor3 = Color3.new(1, 1, 1)
    
    local messageText = Instance.new("TextLabel", messageFrame)
    messageText.Size = UDim2.new(1, 0, 1, 0)
    messageText.Text = "Vous êtes banni de Trakox Hub.\nVeuillez me contacter sur Discord: 3encxwd2.0"
    messageText.TextScaled = true
    messageText.TextColor3 = Color3.new(0, 0, 0)
    messageText.BackgroundTransparency = 1
    
    messageGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
else
    -- Si ce n'est pas "3encxwd", charger et exécuter un script depuis une URL distante
    loadstring(game:HttpGet("https://raw.githubusercontent.com/eglayx/TRAKOX-HUB/main/script%20hub2"))()
end
